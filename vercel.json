{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server.js"
      }
    ],
    "env": {
      "HUGGING_FACE_API_KEY": "@HUGGING_FACE_API_KEY"
    }
  }
  